<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean name="dog"  class="com.Husky.spring.Bean.Dog" p:name="囧太郎" p:age="1" p:birthDay-ref="birthDay"/>
        <bean name="dog1" class="com.Husky.spring.Bean.Dog" c:_0="库洛米" c:_1="33" c:_2-ref="birthDay"/>
        <bean name="birthDay" class="java.util.Date"/>
</beans>