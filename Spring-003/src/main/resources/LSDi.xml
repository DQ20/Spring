       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean name="Person" class="com.Husky.spring.Bean.Person">
    <property name="names">
        <list>
            <value>张三</value>
            <value>张三</value>
            <value>李四</value>
            <value>王五</value>
